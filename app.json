{
  "expo": {
    "name": "CityPop",
    "slug": "CityPop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/CityPop-Splash.png",
    "splash": {
      "image": "./assets/CityPop-Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/CityPop-Splash.png",
        "backgroundColor": "#505A7C"
      }
    },
    "web": {
      "favicon": "./assets/CityPop-Splash.png"
    }
  }
}
